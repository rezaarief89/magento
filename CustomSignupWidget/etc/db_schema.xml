<?xml version="1.0" ?>
<schema xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Setup/Declaration/Schema/etc/schema.xsd">
	<table name="wow_custom_signup" resource="default" engine="innodb" comment="wow_custom_signup Table">
		<column xsi:type="int" name="signup_id" padding="10" unsigned="true" nullable="false" identity="true" comment="Entity Id"/>
		<constraint xsi:type="primary" referenceId="PRIMARY">
			<column name="signup_id"/>
		</constraint>
		<column name="store_id" nullable="true" xsi:type="smallint" comment="Store Id"/>
		<column name="email" nullable="true" xsi:type="varchar" comment="Signup Email" length="100"/>
		<column name="page" nullable="true" xsi:type="varchar" comment="Submit Page" length="100"/>
		<column name="created_at" nullable="true" xsi:type="datetime" comment="Created At"/>
	</table>
</schema>
